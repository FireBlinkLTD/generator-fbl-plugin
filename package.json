{
  "name": "generator-fbl-plugin",
  "version": "0.2.2",
  "description": "FireBlink Logistics plugin project template.",
  "homepage": "https://github.com/FireBlinkLTD/generator-fbl-plugin",
  "author": {
    "name": "Vlad Tkachenko",
    "email": "vlad@fireblink.com",
    "url": "https://www.fireblink.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "fireblink",
    "fbl",
    "deployment",
    "automation",
    "yo",
    "yeoman",
    "generator",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "1.9.1",
    "yeoman-assert": "3.1.1",
    "nsp": "3.2.1",
    "eslint": "5.5.0",
    "prettier": "1.14.3",
    "husky": "1.1.0",
    "lint-staged": "7.3.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-prettier": "2.7.0",
    "eslint-config-xo": "0.25.0",
    "jest": "23.6.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "yeoman-generator": "^3.1.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "FireBlinkLTD/generator-fbl-plugin",
  "license": "MIT"
}
